# vim: set ft=dosini syntax=dosini :
[program:celery_worker]
;directory = 
command = celery -A weps.celery.app worker --beat -l info -Q weps_celery -c 10 -n 'weps.app@%%h' -s var/run/celerybeat-schedule
autostart = true
startsecs = 5
autorestart = true
startretries = 3
stopsignal = TERM
stopwaitsecs = 10
stopasgroup = true
killasgroup = true
redirect_stderr = true
stdout_logfile_maxbytes = 100MB
stdout_logfile_backups = 3
stdout_logfile = var/log/celery_worker.log

;environment=
